{
  "name": "@nasheomirro/stratum",
  "version": "0.1.0",
  "description": "an extremely light-weight wrapper for your tailwindCSS projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasheomirro/stratum.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./presets/*": {
      "style": "./dist/presets/*.css",
      "default": "./dist/presets/*.css"
    },
    "./themes/*": {
      "style": "./dist/themes/*.css",
      "default": "./dist/themes/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "MIT",
  "packageManager": "pnpm@10.6.5",
  "peerDependencies": {
    "tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "sass-embedded": "^1.86.0",
    "tinyglobby": "^0.2.12",
    "vite": "^6.2.3"
  }
}
